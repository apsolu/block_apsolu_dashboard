{{! Liste des onglets }}
<ul class="nav nav-tabs" role="tablist">
  <li class="nav-item">
    <a role="tab" aria-controls="courses" aria-selected="false" class="nav-link" data-toggle="tab" href="#courses">{{#str}}my_courses, block_apsolu_dashboard{{/str}}</a>
  </li>
  <li class="nav-item">
    <a role="tab" aria-controls="rendez-vous" aria-selected="true" class="nav-link active" data-toggle="tab" href="#rendez-vous">{{#str}}my_rendez-vous, block_apsolu_dashboard{{/str}}</a>
  </li>
  {{#teacher}}
  <li class="nav-item">
    <a role="tab" aria-controls="teachings" aria-selected="true" class="nav-link" data-toggle="tab" href="#teachings">{{#str}}my_teachings, block_apsolu_dashboard{{/str}}</a>
  </li>
  {{/teacher}}
  <li class="nav-item">
    <a role="tab" aria-controls="payments" aria-selected="true" class="nav-link" data-toggle="tab" href="#payments">{{#str}}my_payments, block_apsolu_dashboard{{/str}}</a>
  </li>
</ul>

{{! Liste des contenus }}
<div class="tab-content">
  <div id="courses" class="tab-pane fade" role="tabpanel" aria-labelledby="courses-tab">
    {{> block_apsolu_dashboard/courses }}
  </div>
  <div id="rendez-vous" class="tab-pane show active" role="tabpanel" aria-labelledby="rendez-vous-tab">
    {{> block_apsolu_dashboard/rendez-vous }}
  </div>
  {{#teacher}}
  <div id="teachings" class="tab-pane fade" role="tabpanel" aria-labelledby="teachings-tab">
    {{> block_apsolu_dashboard/teachings }}
  </div>
  {{/teacher}}
  <div id="payments" class="tab-pane fade" role="tabpanel" aria-labelledby="payments-tab">
    {{> block_apsolu_dashboard/payments }}
  </div>
</div>
